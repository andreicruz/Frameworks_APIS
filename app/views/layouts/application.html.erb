<!DOCTYPE html>
<html>
  <head>
    <title>FrameworksApis</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <header class="header-web hidden-sm">
    <div class="container menu">
      <div class="logo">
        <%= link_to '', index_index_path, :class => "fa fa-home fa-3x" %>
      </div>
      <div class="links">
        <%= link_to 'Categorias', categorias_path, :class => "dropdown-toggle" %>
      </div>
    </div>
  </header

  <body>
    <%= yield %>
  </body>
</html>
